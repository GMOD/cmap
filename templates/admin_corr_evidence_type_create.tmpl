[% INCLUDE admin_header.tmpl title='Correspondence Evidence Type Create' %]
<a href="admin">Home</a> |
<a href="admin?action=corr_evidence_types_view">View Correspondence Evidence Types</a>
<hr noshade>

[% IF errors.size %]
    <b>There were errors:</b>
    <ul>
        [% FOREACH e=errors %] 
          <li>[% e %]</li> 
        [% END %]
    </ul>
[% END %]

<form action="admin" method="post">
    <table>
        <tr>
            <td>Acc. ID:</th>
            <td><input name="accession_id"></td>
        </tr>

        <tr>
            <td>Evidence Type:</th>
            <td><input name="evidence_type"></td>
        </tr>

        <tr>
            <td>Rank:</th>
            <td><input name="rank"></td>
        </tr>

        <tr>
            <td colspan="2" align="center">
                <input type="hidden" name="action" value="corr_evidence_type_insert">
                <input type="submit" value="Submit">
            </th>
        </tr>
    </table>
</form>
[% INCLUDE admin_footer.tmpl %]
