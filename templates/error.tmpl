[% 
    INCLUDE header.tmpl 
    title='Error'
    no_link=''
    help_anchor=''
%]

<h1>Genetic &amp; Comparative Maps - Error</h1>

<table width="100%">
    <tr>
        <th align="right">Error:</th>
        <td>[% error.text OR 'Undefined error' | html %]</td>
    </tr>
    [% IF debug %]
        <tr>
            <th align="right">File:</th>
            <td>[% error.file %]</td>
        </tr>
        <tr>
            <th align="right">Line:</th>
            <td>[% error.line %]</td>
        </tr>
        <tr>
            <th align="right" valign="top">Stacktrace:</th>
            <td>[% error.stacktrace | html %]</td>
        </tr>
    [% END %]
</table>

[% page.end_body OR '</body>' %]
