CREATE TABLE cmap_xref (
  xref_id integer(11) NOT NULL DEFAULT '0',
  table_name varchar(30) NOT NULL DEFAULT '',
  object_id integer(11) DEFAULT NULL,
  display_order integer(11) NOT NULL DEFAULT '1',
  xref_name varchar(200) NOT NULL DEFAULT '',
  xref_url text NOT NULL,
  PRIMARY KEY (xref_id)
);
