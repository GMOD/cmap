--
-- Table: cmap_attribute
--
CREATE TABLE cmap_attribute (
  attribute_id integer(11) NOT NULL DEFAULT '0',
  table_name varchar(30) NOT NULL DEFAULT '',
  object_id integer(11) NOT NULL DEFAULT '0',
  display_order integer(11) NOT NULL DEFAULT '1',
  is_public tinyint(4) NOT NULL DEFAULT '1',
  attribute_name varchar(200) NOT NULL DEFAULT '',
  attribute_value text NOT NULL,
  INDEX table_name (table_name, object_id, display_order, attribute_name),
  PRIMARY KEY (attribute_id)
);

--
-- Table: cmap_feature_alias
--
CREATE TABLE cmap_feature_alias (
  feature_alias_id integer(11) NOT NULL DEFAULT '0',
  feature_id integer(11) NOT NULL DEFAULT '0',
  alias varchar(255) DEFAULT NULL,
  INDEX feature_id (feature_id),
  INDEX alias (alias),
  PRIMARY KEY (feature_alias_id),
  UNIQUE (feature_id, alias)
);

--
-- Table: cmap_xref
